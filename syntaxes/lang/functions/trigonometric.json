				"parameters": {
					"number": "double precision"
				}
			}
		],
		"asind": [
			{
				"description": "Arc sine, result in degrees",
				"example": "SELECT asind(0.5);",
				"returns": "double precision",
				"parameters": {
					"number": "double precision"
				}
			}
		],
		"atan": [
			{
				"description": "Arc tangent, result in radians",
