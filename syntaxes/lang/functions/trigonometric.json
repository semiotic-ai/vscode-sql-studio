			{
				"description": "Arc tangent of y/x, result in radians",
				"example": "SELECT atan2(1.0, 1.0);",
				"returns": "double precision",
				"parameters": {
					"y": "double precision",
					"x": "double precision"
				}
			}
		],
		"atan2d": [
