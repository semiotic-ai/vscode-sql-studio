{
	"name": "semiotic-sql-studio",
	"displayName": "Semiotic SQL Studio",
	"description": "A SQL interface for The Graph Protocol managed by Semiotic Labs",
	"version": "0.0.1",
	"publisher": "SemioticLabs",
	"engines": {
		"vscode": "^1.86.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "semiotic-sql-studio.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "subgraphs.loadMoreSubgraphs",
				"title": "Load More",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "subgraph-explorer",
					"title": "Subgraph Explorer",
					"icon": "resources/the-graph.svg"
				}
			]
		},
		"views": {
			"subgraph-explorer": [
				{
					"id": "subgraph-available",
					"name": "Available Subgraphs"
				},
				{
					"id": "subgraph-columns",
					"name": "Subgraph columns"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "subgraph-columns",
				"contents": "In order to view an specific subgraph columns, select a SQL enabled subgraph.",
				"when": "workbenchState == empty"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vscode-test",
		"release": "semantic-release",
		"check-format": "prettier --check .",
		"prepare": "husky",
		"docs": "typedoc"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^11.1.0",
		"@semantic-release/git": "^10.0.1",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@types/vscode": "^1.86.0",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.19.1",
		"@vscode/test-cli": "^0.0.4",
		"@vscode/test-electron": "^2.3.9",
		"conventional-changelog-conventionalcommits": "^7.0.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.1",
		"prettier": "^3.2.4",
		"semantic-release": "^23.0.0",
		"semantic-release-stop-before-publish": "^1.0.0",
		"semantic-release-vsce": "^5.6.4",
		"ts-loader": "^9.5.1",
		"typedoc": "^0.25.7",
		"typedoc-theme-hierarchy": "^4.1.2",
		"typescript": "^5.3.3",
		"webpack": "^5.90.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"change-case": "^5.4.1",
		"graphql": "^16.8.1"
	},
	"volta": {
		"node": "20.11.0",
		"npm": "10.2.4"
	}
}
